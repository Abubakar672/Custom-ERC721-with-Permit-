                // Keep dividing `temp` until zero.
                temp := div(temp, 10)
            } {
                // Body of the for loop.
                ptr := sub(ptr, 1)
                mstore8(ptr, add(48, mod(temp, 10)))
            }

            let length := sub(end, ptr)
             ptr := sub(ptr, 32)
            // Store the length.
            mstore(ptr, length)
        }
    }
}
